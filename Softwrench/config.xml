<?xml version='1.0' encoding='utf-8'?>
<widget id="com.softwrenchsolutions.sw4" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <preference name="SplashScreen" value="screen" />
    <preference name="SplashScreenDelay" value="3000" />
    <preference name="StatusBarOverlaysWebView" value="true"/>
        <preference name="StatusBarBackgroundColor" value="#000000"/>
            <preference name="StatusBarStyle" value="lightcontent"/>
    <platform name="android">
              <icon src="res/drawable/icon.png"/>
     </platform>
 
    <name>Softwrench</name>
    <description>
        Maximo Simplified
    </description>
    <author email="info@controltechnologysolutions.com" href="http://www.controltechnologysolutions.com">
        Control Technology Solutions
    </author>
    <feature name="NetworkStatus">
        <param name="ios-package" value="CDVConnection" />
    </feature>
    <feature name="InAppBrowser">
        <param name="ios-package" value="CDVInAppBrowser" />
    </feature>
    <access origin="http://code.jquery.com" subdomains="true" />
    <content src="connect.html" />
    <access origin="*" browserOnly="true"/>
</widget>
